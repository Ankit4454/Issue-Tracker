<link rel="stylesheet" type="text/css" href="/css/home.css">
<h1>All Projects</h1>
<div id="projectList">
    <% if(projects){ 
        for(project of projects){ %>
            <a class="projectItem" href="/project/?id=<%=project.id%>">
                <div>
                    <div class="projectName">
                        <h1><%=project.projectName%></h1>
                    </div>
                    <div class="projectDesc">
                        <p><%=project.author%></p>
                    </div>
                </div>
            </a>
        <% }
    } %>
</div>